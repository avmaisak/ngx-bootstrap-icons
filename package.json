    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jasmine-core": "3.10.1",
