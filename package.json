    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jsdoc": "36.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
